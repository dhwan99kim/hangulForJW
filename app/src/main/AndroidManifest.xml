<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.sophism.hangul">

    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@mipmap/jw_drawing" android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.sophism.hangul.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.sophism.hangul.DrawActivity"/>
    </application>

</manifest>
